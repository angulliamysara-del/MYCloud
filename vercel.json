{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/(api/.*)",
            "destination": "/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/frontend/$1"
        }
    ]
}